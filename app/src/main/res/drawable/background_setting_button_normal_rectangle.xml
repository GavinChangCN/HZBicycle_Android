<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="@color/colorBasicTextNine" />
            <stroke android:width="1dp" android:color="@color/colorBasicLine"></stroke>
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dp" android:color="@color/colorBasicWhite"></stroke>
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="@color/colorBasicLine" />
            <stroke android:width="1dp" android:color="@color/colorBasicWhite"></stroke>
        </shape>
    </item>

</selector>